page.visual_effect :SlideDown, 'event'



page.delay(4.4) do
  page.visual_effect :Pulsate, 'event'
end

page.delay(5.0) do
 page.closeevent
end



page.delay(5.6) do
  page.visual_effect :Highlight, 'logo', 
                     :startcolor => "'#003300'",
                     :endcolor => "'#FFFFCC'",
                     :duration => 1.5
end